{
  "name": "turning",
  "version": "0.1.0-wip.5",
  "description": "A state-based testing framework.",
  "repository": "https://github.com/vilic/turning.git",
  "author": "vilicvane",
  "license": "MIT",
  "scripts": {
    "build:library": "tsc --project src/library",
    "build": "yarn build:library",
    "lint:library": "eslint src/library --ext .ts,.tsx",
    "lint:prettier": "prettier --check \"**/*.{ts,tsx,js,jsx,json,yml,md}\"",
    "lint": "yarn lint:prettier && yarn lint:library",
    "test": "jest"
  },
  "main": "bld/library/index.js",
  "types": "bld/library/index.d.ts",
  "files": [
    "src/library/**/*.ts",
    "bld/library",
    "!*.tsbuildinfo"
  ],
  "dependencies": {
    "chalk": "^3.0.0",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "node-dijkstra": "^2.5.0",
    "prando": "^5.1.2",
    "tslang": "^0.1.21",
    "villa": "^0.3.1"
  },
  "devDependencies": {
    "@magicspace/configs": "^0.2.3",
    "@magicspace/eslint-plugin": "^0.1.16",
    "@types/lodash": "^4.14.149",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^13.7.0",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  }
}
