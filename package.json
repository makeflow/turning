{
  "name": "turning",
  "version": "0.1.0-wip.4",
  "description": "A state-based testing framework.",
  "repository": "https://github.com/vilic/turning.git",
  "author": "vilicvane",
  "license": "MIT",
  "scripts": {
    "build:library": "tsc --project src/library",
    "build": "yarn build:library",
    "lint:library": "tslint --project src/library",
    "lint:test": "tslint --project test",
    "lint-prettier": "prettier --check \"**/*.{ts,tsx,js,jsx,json,yml,md}\"",
    "lint": "yarn lint-prettier && yarn lint:library && yarn lint:test",
    "test": "jest"
  },
  "main": "bld/library/index.js",
  "types": "bld/library/index.d.ts",
  "files": [
    "src/library/**/*.ts",
    "bld/library",
    "!*.tsbuildinfo"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "lodash": "^4.17.11",
    "micromatch": "^4.0.2",
    "node-dijkstra": "^2.5.0",
    "prando": "^5.1.0",
    "tslang": "^0.1.13",
    "villa": "^0.3.1"
  },
  "devDependencies": {
    "@magicspace/configs": "^0.1.47",
    "@types/lodash": "^4.14.123",
    "@types/micromatch": "^3.0.1",
    "@types/node": "^11.13.6",
    "prettier": "^1.17.0",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.4.4"
  }
}
